---
import background from '../assets/rm2000_logo.png';
---

<header>
    <img class="logo" src={background.src} alt="Logo" fetchpriority="high" />
</header>


<style>

 header {
     display: flex;
     justify-content: center;
 }
 .logo {
     width: 210px;
     height: auto;
     object-fit: contain;
     margin: 0;
     padding: 2em;
     z-index: 1;
 }
</style>
